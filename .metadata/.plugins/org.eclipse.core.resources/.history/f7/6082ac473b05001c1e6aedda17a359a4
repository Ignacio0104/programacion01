#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout,NULL); //Hay que ponerlo siempre, para que empiece de cero.
	printf("Esta es la primera clase");

	int numero;
	int suma;

	printf("\n Ingrese el numero: \n");
	scanf("%d",&numero);

	suma=numero+numero;

	printf("La suma del número es: %d ",suma);

	if(numero>17)
	{
		printf("Sos mayor de edad");
	}


	return EXIT_SUCCESS;
}
