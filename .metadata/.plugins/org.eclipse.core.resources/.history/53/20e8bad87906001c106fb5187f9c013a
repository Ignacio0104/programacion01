#include <stdio.h>
#include <stdlib.h>

int main() {

	int numero;
	int acumuladorNumero;
	int promedio;
	int i;

	acumuladorNumero=0;

	for(i=0;i<5;i++)
	{
		printf("Ingrese un número: ");
		scanf("%d",%numero);

	}



	return EXIT_SUCCESS;
}
