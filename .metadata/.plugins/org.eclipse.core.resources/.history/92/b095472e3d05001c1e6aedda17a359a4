	suma=numero+numero;

	printf("La suma del número es: %d ",suma);

	if(numero>17)
	{
		printf("\n Sos mayor de edad");
	}*/

	int numeroUno;
	int numeroDos;
	int resta;

	printf("\n Ingrese el primero numero: \n");
	scanf("%d",&numeroUno);
	printf("\n Ingrese el segundo numero: \n");
	scanf("%d",&numeroDos);

	resta = numeroUno - numeroDos;

	printf("La resta es: %d ", resta);


	return EXIT_SUCCESS;
}
