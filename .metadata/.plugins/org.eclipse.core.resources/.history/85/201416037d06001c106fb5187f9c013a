/*
 ============================================================================
Pedir 5 números y dar como resultado máximo y mínimo
 ============================================================================
 */

#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout,NULL);

	int numero;
	int maximo;
	int minimo;
	int primerNumero;
	int i;

	for(i=0;i<5;i++)
	{
		printf("Ingrese un número: ");
		scanf("%d",&numero);
	}

	return EXIT_SUCCESS;
}
